{
  "name": "crewai-agent",
  "version": "1.0.0",
  "description": "CrewAI conversational agent with Ollama integration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/interactive-agent.ts",
    "demo": "tsx src/simple-agent.ts",
    "crewai": "tsx src/index.ts",
    "interactive": "tsx src/interactive-agent.ts",
    "dev": "tsx --watch src/interactive-agent.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "crewai-js": "github:clevaway/crewai-js",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "langchain": "^0.3.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "typescript": "^5.3.0",
    "ts-jest": "^29.1.0",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "crewai",
    "ollama",
    "langchain",
    "ai",
    "conversational-agent"
