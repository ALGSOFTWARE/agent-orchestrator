# Configurações do Gatekeeper API
# Copie este arquivo para .env e ajuste os valores

# Database
MONGODB_URI=mongodb://localhost:27017/mit_logistics
DATABASE_NAME=mit_logistics

# API Settings
API_HOST=0.0.0.0
API_PORT=8001
DEBUG=True

# JWT Settings
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# External Services
CREWAI_BASE_URL=http://localhost:8000

# Logging
LOG_LEVEL=INFO